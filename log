3287 silly resolved     repository:
3287 silly resolved      { type: 'git',
3287 silly resolved        url: 'http://github.com/substack/node-optimist.git' },
3287 silly resolved     keywords:
3287 silly resolved      [ 'argument',
3287 silly resolved        'args',
3287 silly resolved        'option',
