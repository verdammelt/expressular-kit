1771 silly resolved     license: 'MIT',
1771 silly resolved     dependencies: { 'is-promise': '~1' },
1771 silly resolved     devDependencies:
1771 silly resolved      { 'promises-aplus-tests': '*',
1771 silly resolved        'mocha-as-promised': '~1.2.1',
